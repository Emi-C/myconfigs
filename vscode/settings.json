{
    "editor.minimap.enabled": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "extensions.ignoreRecommendations": false,
    "gitlens.keymap": "alternate",
    "[javascript]": {
        "editor.formatOnSave": true
    },
    "csscomb.formatOnSave": true,
    "window.zoomLevel": 0,
    "gitlens.historyExplorer.enabled": true,
    "workbench.colorTheme": "Popping and Locking",
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "workbench.colorCustomizations": {
        "activityBar.background": "#1d1a19",
        "activityBar.foreground": "#f2e5bc",
        "activityBarBadge.background": "#d79921",
        "activityBarBadge.foreground": "#f9f5d7",

        "sideBar.background": "#1d1a19",
        "sideBar.foreground": "#fbf1c7",
        "sideBarSectionHeader.background": "#362e24",
        "list.activeSelectionBackground": "#423b31",
        "list.inactiveSelectionBackground": "#423b31",
        "list.hoverBackground": "#423b31",
        "list.focusBackground": "#423b31",

        "titleBar.activeBackground": "#1d1a19",
        "titleBar.activeForeground": "#f2e5bc",

        "tab.inactiveBackground": "#1d1a19",
        "tab.activeBackground": "#362e24",
        "editorGroupHeader.tabsBackground": "#1d1a19",

        "focusBorder": "#8c683e",
        "foreground": "#f2e5bc",

        "badge.background": "#886b45",

        "statusBar.background": "#1d1a19",
        "statusBar.noFolderBackground": "#1d1a19",
        "statusBar.foreground": "#f2e5bc",

        "tab.activeForeground": "#f2e5bc",

        "input.background": "#211d18",
        "input.border": "#362f27",

        "editor.foreground": "#f2e5bc",
        "editor.background": "#25221e",
        "editor.lineHighlightBackground": "#363127",
        "editorWidget.background": "#363127",


        "editor.selectionBackground": "#ffffff22",
        "editor.inactiveSelectionBackground": "#ffffff22",
        "editor.selectionHighlightBackground": "#FFFFBE55",

        "editor.wordHighlightStrongBackground": "#FFFFBE55",
        "editor.wordHighlightBackground": "#ffffff22",

        "editorCursor.foreground": "#f42c3e",

        "editor.findMatchBackground": "#ffffff22",
        "editor.findMatchHighlightBackground": "#FFFFBE55",
        "editor.findRangeHighlightBackground": "#ffffff22",

        "peekViewEditor.background": "#362e24",
        "peekViewResult.background": "#362e24",
        "peekViewResult.selectionBackground": "#8c683e",
        "peekView.border": "#8c683e",

        "button.background": "#d79921",
        "button.foreground": "#f9f5d7",

        "dropdown.background": "#1d1a19",

        "extensionButton.prominentBackground": "#599442",

        "gitDecoration.modifiedResourceForeground": "#fabd2f",
        "gitDecoration.untrackedResourceForeground": "#7ec16e",
        "gitDecoration.ignoredResourceForeground": "#8c683e",
        "gitDecoration.conflictingResourceForeground": "#f42c3e",

        "terminal.ansiBlack": "#1d2021",
        "terminal.ansiBrightBlack": "#928374",
        "terminal.ansiRed": "#cc241d",
        "terminal.ansiBrightRed": "#f42c3e",
        "terminal.ansiGreen": "#98971a",
        "terminal.ansiBrightGreen": "#b8bb26",
        "terminal.ansiYellow": "#d79921",
        "terminal.ansiBrightYellow": "#fabd2f",
        "terminal.ansiBlue": "#458588",
        "terminal.ansiBrightBlue": "#99c6ca",
        "terminal.ansiMagenta": "#b16286",
        "terminal.ansiBrightMagenta": "#d3869b",
        "terminal.ansiCyan": "#689d6a",
        "terminal.ansiBrightCyan": "#7ec16e",
        "terminal.ansiWhite": "#a89984",
        "terminal.ansiBrightWhite": "#ebdbb2",
        "terminal.foreground": "#ebdbb2",
        "terminal.background": "#211d18",



    },
    "editor.tokenColorCustomizations":{
        "textMateRules": [
            {
                "scope": "string.escape",
                "settings": {
                    "foreground": "#fcea4e"
                }
            },
            {
                "scope": "storage",
                "settings": {
                    "foreground": "#f1c891"
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#f36363"
                }
            },
            {
                "scope": "storage.type.class",
                "settings": {
                    "foreground": "#ce7398"
                }
            },
            {
                "scope": "punctuation.quasi",
                "settings": {
                    "foreground": "#c2ce1f"
                }
            },
            {
                "name": "Comments",
                "scope": ["comment", "punctuation.definition.comment"],
                "settings": {
                  "foreground": "#beaa92",
                  "fontStyle": "italic"
                }
              },
        ]
    }

}